{
  "uuids": {
    "algorithm": "sequential",
    "max_count": "1000"
  },
  "cors": {
    "credentials": true,
    "headers": "accept, authorization, content-type, origin, referer",
    "methods": "GET, PUT, POST, HEAD, DELETE",
    "origins": "http://localhost:4200"
  },
  "chttpd": {
    "backlog": "512",
    "bind_address": "0.0.0.0",
    "max_db_number_for_dbs_info_req": "100",
    "port": "5984",
    "prefer_minimal": "Cache-Control, Content-Length, Content-Range, Content-Type, ETag, Server, Transfer-Encoding, Vary",
    "require_valid_user": "false",
    "server_options": "[{recbuf, undefined}]",
    "socket_options": "[{sndbuf, 262144}, {nodelay, true}]"
  },
  "attachments": {
    "compressible_types": "text/*, application/javascript, application/json, application/xml",
    "compression_level": "8"
  },
  "query_server_config": {
    "os_process_limit": "100",
    "reduce_limit": "true"
  },
  "vendor": {
    "name": "The Apache Software Foundation"
  },
  "smoosh.ratio_views": {
    "min_priority": "2.0"
  },
  "purge": {
    "users_db_security_editable": "false"
  },
  "feature_flags": {
    "partitioned||*": "true"
  },
  "replicator": {
    "connection_timeout": "30000",
    "http_connections": "20",
    "interval": "60000",
    "max_churn": "20",
    "max_jobs": "500",
    "retries_per_request": "5",
    "socket_options": "[{keepalive, true}, {nodelay, false}]",
    "ssl_certificate_max_depth": "3",
    "startup_jitter": "5000",
    "verify_ssl_certificates": "false",
    "worker_batch_size": "500",
    "worker_processes": "4"
  },
  "ssl": {
    "port": "6984"
  },
  "log": {
    "file": "./logs/log.txt",
    "level": "info",
    "writer": "file"
  },
  "indexers": {
    "couch_mrview": "true"
  },
  "couch_peruser": {
    "database_prefix": "userdb-",
    "delete_dbs": "false",
    "enable": "false"
  },
  "httpd": {
    "allow_jsonp": "false",
    "authentication_handlers": "{couch_httpd_auth, cookie_authentication_handler}, {couch_httpd_auth, default_authentication_handler}",
    "bind_address": "127.0.0.1",
    "enable_cors": true,
    "enable_xframe_options": "true",
    "max_http_request_size": "4294967296",
    "port": "5986",
    "secure_rewrites": "true",
    "socket_options": "[{sndbuf, 262144}]"
  },
  "ioq.bypass": {
    "compaction": "false",
    "os_process": "true",
    "read": "true",
    "shard_sync": "true",
    "view_update": "true",
    "write": "true"
  },
  "ioq": {
    "concurrency": "10",
    "ratio": "0.01"
  },
  "smoosh.ratio_dbs": {
    "min_priority": "2.0"
  },
  "csp": {
    "enable": "true"
  },
  "couch_httpd_auth": {
    "allow_persistent_cookies": "true",
    "auth_cache_size": "50",
    "authentication_db": "_users",
    "authentication_redirect": "/_utils/session.html",
    "iterations": "10",
    "require_valid_user": "false",
    "secret": "5cbc52d1e881fc0a0e09b5a678621ec7",
    "timeout": "600"
  },
  "couchdb_engines": {
    "couch": "couch_bt_engine"
  },
  "couchdb": {
    "attachment_stream_buffer_size": "4096",
    "changes_doc_ids_optimization_threshold": "100",
    "database_dir": "./data",
    "default_engine": "couch",
    "default_security": "admin_only",
    "file_compression": "snappy",
    "max_dbs_open": "500",
    "max_document_size": "8000000",
    "os_process_timeout": "5000",
    "uuid": "dab584f86a68f54b684fffef6f02a08c",
    "view_index_dir": "./data"
  }
}
